x1 = int(input())
y1 = int(input())
x2 = int(input())
y2 = int(input())
if x1 % 2 == 0 and y1 % 2 == 0:
    if x2 % 2 == 0 and y2 % 2 == 0:
        print ('yes')
    elif x2 % 2 != 0 and y2 % 2 != 0:
        print ('yes')
    else:
        print ('no')
if x1 % 2 != 0 or y1 % 2 != 0:
    if x2 % 2 != 0 and y2 % 2 != 0:
        print ('no')
    elif x2 % 2 != 0 or y2 % 2 != 0:
        print ('yes')
